{
	"schemaVersion": 1,
	"id": "portalcraft",
	"version": "${version}",
	"name": "PortalCraft",
	"description": "PortalCraft aims to emulate the portal 1 and portal 2 games in minecraft.",
	"authors": [
		"Dani E"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/portalcraft/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.portalcraft.PortalCraft"
		],
		"fabric-datagen": [
			"com.portalcraft.PortalCraftDataGenerator"
		],
		"client": [
			"com.portalcraft.PortalCraftClient"
		]
	},
	"mixins": [
		"portalcraft.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.4",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}